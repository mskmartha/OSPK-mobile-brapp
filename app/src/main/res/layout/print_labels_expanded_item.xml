<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="printLabelSubItemUi"
            type="com.albertsons.acupick.ui.staging.print.PrintLabelsSubItemUi" />

    </data>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/labelItemsListRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/drawable_radius_bottom_4dp"
        android:clipToPadding="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/item_print_labels_item" />
</layout>